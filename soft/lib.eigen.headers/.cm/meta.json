{
  "auto_detect": "yes",
  "customize": {
    "add_include_path": "yes",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_EIGEN_HEADERS",
    "limit_recursion_dir_search": {
      "linux": 4,
      "win": 4
    },
    "skip_add_to_bin": "yes",
    "skip_add_to_path": "yes",
    "soft_file": {
      "linux": "install/share/eigen3/cmake/Eigen3Config.cmake",
      "win": "install\\share\\eigen3\\cmake\\Eigen3Config.cmake"
    }
  },
  "soft_name": "EIGEN library",
  "tags": [
    "lib",
    "eigen",
    "headers"
  ]
}


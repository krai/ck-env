{
  "auto_detect": "yes",
  "customize": {
    "detect_preprocessed_ext": "rgbf32",
    "detect_data_type": "float32",
    "detect_bytes_per_pixel": "12",
    "detect_file_index_name": "image_list.txt",
    "env_prefix": "CK_ENV_DATASET_IMAGENET_PREPROCESSED",
    "limit_recursion_dir_search": {
      "linux": 3,
      "win": 3
    },
    "soft_file_universal": "ILSVRC2012_val_00000001.rgbf32"
  },
  "deps": {},
  "soft_name": "ImageNet dataset (preprocessed float32)",
  "tags": [
    "dataset",
    "imagenet",
    "preprocessed",
    "rgbf32"
  ],
  "variations": {
    "layout.nhwc": {
      "extra_env": {
        "_LAYOUT": "NHWC"
      },
      "on_by_default": "yes"
    },
    "layout.nchw": {
      "extra_env": {
        "_LAYOUT": "NCHW"
      },
      "on_by_default": "no"
    }
}
